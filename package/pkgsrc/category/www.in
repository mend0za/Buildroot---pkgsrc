	  my ( $self, $c ) = @_;
	  
	  my $x : Stashed;
	  my %y : Stashed;
	  
	  $x = 100;
	  
	  do_something( $c->stash->{x} );    # 100
	  
	  $c->forward("gorch");
	  }

